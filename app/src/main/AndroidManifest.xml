<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="swapart.martin.swapartmockup" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".signupActivity" >
        </activity>
        <activity
            android:name=".TermsConditionsActivity"
            android:label="@string/title_activity_terms_conditions" >
        </activity>
        <activity
            android:name=".GalleryActivity"
            android:label="@string/title_activity_gallery" >
        </activity>
        <activity
            android:name=".AddArtMenuActivity"
            android:label="@string/title_activity_add_art_menu" >
        </activity>
        <activity
            android:name=".EditArtActivity"
            android:label="@string/title_activity_edit_art" >
        </activity>
        <activity
            android:name=".SearchArtActivity"
            android:label="@string/title_activity_search_art" >
        </activity>
    </application>

</manifest>
